package org.nhs.patientinfo.process.builders;

import org.nhs.patientinfo.dao.beans.PatientInfoDAORes;
import org.nhs.patientinfo.process.valuebeans.PatientInfoProcessVbRes;

/**
 * @author: Stech
 * @Date     :  Feb 23, 2018
 * Description:  
 */
public class PatientInfoProcessResBuilder {

	/**
	 * @param daoResp
	 * @return
	 */
	public PatientInfoProcessVbRes buildvbResp(PatientInfoDAORes daoResp) {
		// TODO Auto-generated method stub
		PatientInfoProcessVbRes vbResp=new PatientInfoProcessVbRes();
		vbResp.setRespCode(daoResp.getRespCode());
		vbResp.setRespMsg(daoResp.getRespMsg());
		return vbResp;
		
	}

	
}
