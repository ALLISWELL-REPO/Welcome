<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<%session.invalidate();%>
<body bgcolor="lightblue">
	<center style="margin-top: 100px;">
		<h1 style="color: green;">Welcome to O2Login Page</h1>
		<form action="o2login" method="post">
			<fieldset
				style="height: 120px; width: 80px; color: darkblue; border-color: lightgreen">
				<table>

					<tr>
						<td>AccountNo:</td>
						<td><input type="text" name="accNo" required
							style="height: 20px; width: 160px; font-size: 14px; color: darkblue;" ></td>
					</tr>

					<tr>
						<td>MobileNo:</td>
						<td><input type="number" name="mobNo" required
							style="height: 20px; width: 160px; font-size: 14px; color: darkblue;"></td>
					</tr>

					<tr>
						<td>Password:</td>
						<td><input type="password" name="pass" required
							style="height: 20px; width: 160px; font-size: 18px; color: darkblue;"></td>
					</tr>

					<tr>
						<td></td>
						<td><input type="submit" value="Login"
							style="height: 23px; width: 165px; font-size: 16px; color: darkblue;"></td>
					</tr>

				</table>
			</fieldset>
		</form>
	</center>
</body>