package com.citybank.statementdtls_schema.req;

import com.fasterxml.jackson.annotation.JsonRootName;

@JsonRootName("CustomerDetailsType")
public class CustomerDetailsType {

    protected String cardnum;
    protected String cvvnum;
    protected String expdate;
    protected String nameoncard;

 
    public String getCardnum() {
        return cardnum;
    }

    public void setCardnum(String value) {
        this.cardnum = value;
    }

    public String getCvvnum() {
        return cvvnum;
    }

    public void setCvvnum(String value) {
        this.cvvnum = value;
    }

    public String getExpdate() {
        return expdate;
    }

    public void setExpdate(String value) {
        this.expdate = value;
    }

    public String getNameoncard() {
        return nameoncard;
    }

    public void setNameoncard(String value) {
        this.nameoncard = value;
    }

}
